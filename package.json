{
  "name": "appointments",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "babel-node --no-babelrc --presets @babel/preset-env ./src/server/index.js",
    "build": "npm run clear && cross-env NODE_ENV=production webpack --mode production",
    "dev": "cross-env NODE_ENV=development concurrently --kill-others \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "webpack --watch --mode development",
    "dev:server": "nodemon --watch ./src/server --exec babel-node ./src/server/index.js",
    "clear": "rimraf public",
    "test": "npm run test:server && npm run test:client",
    "test:watch": "concurrently --kill-others \"npm run test:watch:server\" \"npm run test:watch:client\"",
    "test:client": "cross-env NODE_ENV=test jest --config=./jest.client.config.js",
    "test:server": "cross-env NODE_ENV=test jest --config=./jest.server.config.js",
    "test:watch:client": "cross-var $npm_execpath run test:client -- --watchAll",
    "test:watch:server": "cross-var $npm_execpath run test:server -- --watchAll",
    "docker": "docker-compose up"
  },
  "keywords": [
    "health",
    "appointment"
  ],
  "author": "Pedro Alves",
  "license": "MIT",
  "dependencies": {
