{
  "name": "appointments",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "babel-node --no-babelrc --presets @babel/preset-env ./src/server/index.js",
    "build": "npm run clear && cross-env NODE_ENV=production webpack --mode production",
    "dev": "cross-env NODE_ENV=development concurrently --kill-others \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "webpack --watch --mode development",
    "dev:server": "nodemon --watch ./src/server --exec babel-node --no-babelrc --presets @babel/preset-env ./src/server/index.js",
    "clear": "rimraf public",
    "test": "cross-env NODE_ENV=test npm run test:server && npm run test:client",
    "test:watch": "cross-env NODE_ENV=test concurrently --kill-others \"npm run test:watch:server\" \"npm run test:watch:client\"",
    "test:client": "jest --config=./jest.client.config.js",
    "test:server": "jest --config=./jest.server.config.js",
    "test:watch:client": "jest --config=./jest.client.config.js --watchAll",
    "test:watch:server": "jest --config=./jest.server.config.js --watchAll",
    "docker": "docker-compose up"
  },
  "keywords": [
    "health",
    "appointment"
  ],
  "author": "Pedro Alves",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.3.4",
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/lab": "^3.0.0-alpha.30",
    "@sentry/browser": "^4.6.4",
    "apollo-boost": "^0.3.1",
    "apollo-client": "^2.5.1",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "jsonwebtoken": "^8.5.1",
    "jss": "^9.8.7",
    "jss-rtl": "^0.2.3",
    "lodash.pick": "^4.4.0",
    "moment": "^2.24.0",
    "mongoose": "^5.4.19",
    "mongoose-unique-validator": "^2.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
